--

CREATE VIEW public."konumBilgisi" AS
 SELECT il."ilKodu",
    il."ilAdi",
    ilce."ilceKodu",
    ilce."ilceAdi",
    mahalle."mahalleKodu",
    mahalle."mahalleAdi"
   FROM ((public.il
     JOIN public.ilce ON ((il."ilKodu" = ilce."ilNo")))
     JOIN public.mahalle ON ((il."ilKodu" = mahalle."ilNo")));


