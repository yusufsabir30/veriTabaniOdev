CREATE FUNCTION public."ilUzunluguTrigger"() RETURNS trigger
    LANGUAGE plpgsql
    AS $$ 
DECLARE 
iluzunlugu INTEGER;
BEGIN 
iluzunlugu := (SELECT LENGTH("ilAdi") FROM "il" ORDER BY "siraNumarasi" DESC LIMIT 1);

UPDATE "ilUzunlugu" SET "uzunlukMiktari" = "uzunlukMiktari"+"ilUzunlugu";
RETURN NEW;
END;
$$;


